@{
    ViewBag.Title = "Java Home Page";
}


<script type="text/javascript">
    function JSFunction() {



        location.href = '@Url.Action("Index", "Questions")';

    }
</script>

<!DOCTYPE HTML>
<html>


<head>
    <title>@ViewBag.Title  - My ASP.NET Application</title>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <meta name="description" content="" />
    <meta name="keywords" content="" />
    <!--5grid-->
    <script src="css/5grid/viewport.js"></script><!--[if lt IE 9]><script src="css/5grid/ie.js"></script><![endif]-->
    <link rel="stylesheet" href="css/5grid/core.css" />
    <link rel="stylesheet" href="css/style.css" />
    <!--[if IE 9]><link rel="stylesheet" href="css/style-ie9.css" /><![endif]-->
</head>

<body>
    <!-- ********************************************************* -->
    <header id="header_outer">
        <div class="container">
            <div class="header_section">
                <div class="logo">
                    <a href="javascript:void(0)"><img class="img1" src="~/img/CGLogo.png" alt=""></a>
                </div>
                <nav class="nav" id="nav1">
                    <ul class="">
                        <li><a href="@Url.Action("Index","Home")" class="contact_btn">Home</a></li>
                    </ul>

                </nav>

            </div>
        </div>
    </header>
    <div id="banner-wrapper">
        <div class="5grid">
            <div class="12u-first">

                <div id="banner">

                </div>

            </div>
        </div>
    </div>
    <div class="row">

        <center><img class="img" src="~/img/DotNetImage.jpg" height="500" width="800" /></center>
        <br />
        <center><h1><input type="button" class="contact_btn" onclick="JSFunction()" value="Next"></h1></center>
    </div>
</body>
</html>
